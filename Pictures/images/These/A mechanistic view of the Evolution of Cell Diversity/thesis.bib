@article{Bloom05,
	Author = {Bloom, Jesse D. and Silberg, Jonathan J. and Wilke, Claus O. and Drummond, D. Allan and Adami, Christoph and Arnold, Frances H.},
	Date = {2005/01/18},
	Date-Added = {2021-02-16 15:15:03 +0100},
