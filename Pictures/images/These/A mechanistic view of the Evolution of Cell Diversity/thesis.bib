@article{Dong95,
	Author = {Dong, H and Nilsson, L and Kurland, C G},
	Date = {1995/03/01},
	Date-Added = {2021-02-15 17:52:03 +0100},
	Date-Modified = {2021-02-15 17:52:15 +0100},
