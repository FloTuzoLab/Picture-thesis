	Isbn = {1573-4919},
	Journal = {Molecular and Cellular Biochemistry},
	Number = {1},
	Pages = {5--12},
	Title = {On the origin of intracellular compartmentation and organized metabolic systems},
	Ty = {JOUR},
	Url = {https://doi.org/10.1023/B:MCBI.0000009855.14648.2c},
	Volume = {256},
	Year = {2004},
