	Keywords = {Enzyme, efficiency, selection, evolution},
	Month = {3/18/2020},
	Number = {12},
	Pages = {2537--2546},
	Title = {Evolutionary Constraint and Adaptation in the Metabolic Network of Drosophila},
	Ty = {JOUR},
