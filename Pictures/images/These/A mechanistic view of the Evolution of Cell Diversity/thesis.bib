	Doi = {10.1007/s10955-018-1975-3},
	Id = {Weinreich2018},
	Isbn = {1572-9613},
	Journal = {Journal of Statistical Physics},
	Number = {1},
	Pages = {208--225},
	Title = {The Influence of Higher-Order Epistasis on Biological Fitness Landscape Topography},
