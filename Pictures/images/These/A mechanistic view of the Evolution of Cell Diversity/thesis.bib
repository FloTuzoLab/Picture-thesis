@article{Solopova14,
	Author = {Solopova, Ana and van Gestel, Jordi and Weissing, Franz J. and Bachmann, Herwig and Teusink, Bas and Kok, Jan and Kuipers, Oscar P.},
	Date = {2014/05/20},
	Date-Added = {2021-05-05 12:47:43 +0200},
