@article{Schlichting02,
	Annote = {doi: 10.1046/j.1525-142X.2003.03015.x},
	Author = {Schlichting, Carl D.},
	Booktitle = {Evolution \& Development},
	Da = {2003/01/01},
	Date = {2002/12/19},
