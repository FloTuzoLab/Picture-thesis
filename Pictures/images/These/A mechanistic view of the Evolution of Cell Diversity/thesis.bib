	Month = {4/29/2021},
	Number = {5},
	Pages = {1092--1103},
	Title = {Codon Usage Bias in Animals: Disentangling the Effects of Natural Selection, Effective Population Size, and GC-Biased Gene Conversion},
	Ty = {JOUR},
	Url = {https://doi.org/10.1093/molbev/msy015},
