	Isbn = {0022-2836},
	Journal = {Journal of Molecular Biology},
	Keywords = {protein stability; mutational robustness; computational biophysics; ΔΔdistributions; protein models},
	Number = {5},
	Pages = {1318--1332},
	Title = {The Stability Effects of Protein Mutations Appear to be Universally Distributed},
	Ty = {JOUR},
