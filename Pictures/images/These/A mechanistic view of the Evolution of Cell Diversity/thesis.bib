@article{Ford20,
	Annote = {https://doi.org/10.1111/cla.12421},
	Author = {Ford, Colby T. and Zenarosa, Gabriel Lopez and Smith, Kevin B. and Brown, David C. and Williams, John and Janies, Daniel},
	Booktitle = {Cladistics},
	Da = {2020/08/01},
	Date = {2020/08/01},
	Date-Added = {2021-04-29 20:08:53 +0200},
