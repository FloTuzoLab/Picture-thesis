	Journal = {Nature Communications},
	Keywords = {Flux estimations, Flux coordination, glucose, amino acids},
	Number = {1},
	Pages = {3354},
	Title = {Regulatory mechanisms underlying coordination of amino acid and glucose catabolism in Escherichia coli},
	Ty = {JOUR},
	Url = {https://doi.org/10.1038/s41467-019-11331-5},
	Volume = {10},
