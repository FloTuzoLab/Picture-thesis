@article{Lycus18,
	Author = {Lycus, Pawel and Soriano-Laguna, Manuel Jes{\'u}s and Kjos, Morten and Richardson, David John and Gates, Andrew James and Milligan, Daniel Aleksanteri and Frosteg{\aa}rd, {\AA}sa and Bergaust, Linda and Bakken, Lars Reier},
	Date = {2018/11/13},
	Date-Added = {2021-08-06 14:45:13 +0200},
