	Isbn = {2045-2322},
	Journal = {Scientific Reports},
	Keywords = {transporter, hxt, facilitated diffusion, efflux hypothesis},
	Number = {1},
	Pages = {5576},
	Title = {Low affinity uniporter carrier proteins can increase net substrate uptake rate by reducing efflux},
	Ty = {JOUR},
