@article{Achtman08,
	Abstract = {According to the biological species concept, the existence of species in many animals and some plants is ensured by the cohesive evolutionary forces that result from pre- and post-zygotic barriers between eukaryotic species.Various concepts have been suggested for microbial species, but none have been generally accepted, possibly because all of these concepts include methodological considerations.We propose that microbes that form distinct groups owing to a particular cohesive force are metapopulation lineages and thus form species, whereas microorganisms without limits imposed by a cohesive force do not.The population structures of pathogens such as Yersinia pestis and Neisseria meningitidis are examined to illustrate evolutionary mechanisms of speciation.We examine how horizontal gene transfer and recombination affect the concept of species in Bacteria and Archaea, and conclude that neither precludes classification into species.We speculate that the adoption of a method-free species concept would free microbiologists to choose the techniques that best suit their organism of choice when attempting to classify an organism.},
	Author = {Achtman, Mark and Wagner, Michael},
	Da = {2008/06/01},
	Date-Added = {2021-08-21 17:29:25 +0200},
