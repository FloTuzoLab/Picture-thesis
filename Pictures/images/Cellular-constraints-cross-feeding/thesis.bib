@article{Yang20,
	Annote = {https://doi.org/10.1002/pro.3901},
	Author = {Yang, Gloria and Miton, Charlotte M. and Tokuriki, Nobuhiko},
	Booktitle = {Protein Science},
	Da = {2020/08/01},
