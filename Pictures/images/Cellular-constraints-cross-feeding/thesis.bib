@article{Bell01,
	Author = {Bell, Graham},
	Date = {2001/09/28},
	Date-Added = {2021-08-14 18:40:14 +0000},
	Date-Modified = {2021-08-14 18:40:20 +0000},
	Doi = {10.1126/science.293.5539.2413},
	Journal = {Science},
	Journal1 = {Science},
	Lp = {2418},
	Month = {09},
	N2 = {The central themes of community ecology---distribution, abundance, and diversity---display strongly marked and very general patterns. These include the log-normal distribution of abundance, the relation between range and abundance, the species-area law, and the turnover of species composition. Each pattern is the subject of a large literature that interprets it in terms of ecological processes, typically involving the sorting of differently specialized species onto heterogeneous landscapes. All of these patterns can be shown to arise, however, from neutral community models in which all individuals have identical properties, as the consequence of local dispersal alone. This implies, at the least, that functional interpretations of these patterns must be reevaluated. More fundamentally, neutral community models provide a general theory for biodiversity and conservation biology capable of predicting the fundamental processes and patterns of community ecology.},
	Number = {5539},
	Pages = {2413},
	Title = {Neutral Macroecology},
	Ty = {JOUR},
