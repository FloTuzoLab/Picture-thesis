	Eprint = {https://www.pnas.org/content/108/29/11815.full.pdf},
	Issn = {0027-8424},
	Journal = {Proceedings of the National Academy of Sciences},
	Keywords = {Diffusion, crowding, macromolecules, protein},
	Number = {29},
	Pages = {11815--11820},
	Publisher = {National Academy of Sciences},
	Title = {Protein self-diffusion in crowded solutions},
	Url = {https://www.pnas.org/content/108/29/11815},
	Volume = {108},
	Year = {2011},
	Bdsk-Url-1 = {https://www.pnas.org/content/108/29/11815},
