@article{Erwin11,
	Author = {Erwin, Douglas H. and Laflamme, Marc and Tweedt, Sarah M. and Sperling, Erik A. and Pisani, Davide and Peterson, Kevin J.},
	Date = {2011/11/25},
	Date-Added = {2021-08-22 19:25:35 +0200},
