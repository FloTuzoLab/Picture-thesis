@article{Veening08a,
	Author = {Veening, Jan-Willem and Stewart, Eric J. and Berngruber, Thomas W. and Taddei, Fran{\c c}ois and Kuipers, Oscar P. and Hamoen, Leendert W.},
	Date = {2008/03/18},
	Date-Added = {2021-08-06 13:36:04 +0200},
