@article{Carlin16,
	Author = {Carlin, Dylan Alexander and Caster, Ryan W. and Wang, Xiaokang and Betzenderfer, Stephanie A. and Chen, Claire X. and Duong, Veasna M. and Ryklansky, Carolina V. and Alpekin, Alp and Beaumont, Nathan and Kapoor, Harshul and Kim, Nicole and Mohabbot, Hosna and Pang, Boyu and Teel, Rachel and Whithaus, Lillian and Tagkopoulos, Ilias and Siegel, Justin B.},
	Date = {2016/01/27},
	Date-Added = {2020-07-16 20:41:34 +0200},
