@article{Niehaus20,
	Annote = {https://doi.org/10.1111/febs.15284},
	Author = {Niehaus, Thomas D. and Hillmann, Katie B.},
	Booktitle = {The FEBS Journal},
	Da = {2020/04/01},
	Date = {2020/04/01},
	Date-Added = {2021-02-18 20:19:54 +0100},
