	Volume = {7},
	Year = {2011},
	Year1 = {2011},
	Bdsk-Url-1 = {https://doi.org/10.1038/msb.2011.34}}

@article{OBrien13,
	Annote = {https://doi.org/10.1038/msb.2013.52},
	Author = {O'Brien, Edward J and Lerman, Joshua A and Chang, Roger L and Hyduke, Daniel R and Palsson, Bernhard {\O}},
	Booktitle = {Molecular Systems Biology},
	Da = {2013/01/01},
