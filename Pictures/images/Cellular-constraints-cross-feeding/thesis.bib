	Month = {7/30/2021},
	Number = {2},
	Pages = {1049--1079},
	Title = {Genotypic Complexity of Fisher's Geometric Model},
	Ty = {JOUR},
	Url = {https://doi.org/10.1534/genetics.116.199497},
	Volume = {206},
	Year = {2017},
