	Isbn = {0040-5809},
	Journal = {Theoretical Population Biology},
	Keywords = {Fisher's geometric model; Steady state; Genetic load; Fixed-drift-load; Mutation-selection-drift balance; Nearly neutral evolution},
	Number = {1},
	Pages = {30--34},
	Title = {An exact steady state solution of Fisher's geometric model and other models},
