	L2 = {https://www.ncbi.nlm.nih.gov/pmc/PMC5031622/},
	Month = {10},
	Number = {1706},
	Pages = {20150538},
	Publisher = {The Royal Society},
	Title = {What does the geography of parthenogenesis teach us about sex?},
	Ty = {JOUR},
