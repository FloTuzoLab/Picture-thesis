@article{Nagy18,
	Annote = {doi: 10.1111/brv.12418},
	Author = {Nagy, L{\'a}szl{\'o}G. and Kov{\'a}cs, G{\'a}bor M. and Krizs{\'a}n, Krisztina},
	Booktitle = {Biological Reviews},
	Da = {2018/11/01},
	Date = {2018/04/19},
	Date-Added = {2018-11-19 14:32:46 +0100},
