	Number = {1},
	Pages = {145},
	Title = {Hidden paths to endless forms most wonderful: Complexity of bacterial motility shapes diversification of latent phenotypes},
	Ty = {JOUR},
	Url = {https://doi.org/10.1186/s12862-020-01707-3},
	Volume = {20},
