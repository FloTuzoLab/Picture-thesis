	Year1 = {2002},
	Bdsk-Url-1 = {https://doi.org/10.1002/prot.10016}}

@article{Yang12,
	Author = {Yang, Jian-Rong and Liao, Ben-Yang and Zhuang, Shi-Mei and Zhang, Jianzhi},
	Date = {2012/04/03},
