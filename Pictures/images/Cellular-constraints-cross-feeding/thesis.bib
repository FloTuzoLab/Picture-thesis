@article{Kittelmann18,
	Author = {Kittelmann, Sebastian and Buffry, Alexandra D. and Franke, Franziska A. and Almudi, Isabel and Yoth, Marianne and Sabaris, Gonzalo and Couso, Juan Pablo and Nunes, Maria D. S. and Frankel, Nicol{\'a}s and G{\'o}mez-Skarmeta, Jos{\'e}Luis and Pueyo-Marques, Jose and Arif, Saad and McGregor, Alistair P.},
