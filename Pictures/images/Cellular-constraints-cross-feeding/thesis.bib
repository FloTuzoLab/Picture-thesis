	Keywords = {Enzymes/genetics; *Genes, Dominant; *Genetic Variation; Kinetics; Mathematics; *Models, Genetic; Molecular Conformation; Mutation},
	L2 = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC1214416/},
