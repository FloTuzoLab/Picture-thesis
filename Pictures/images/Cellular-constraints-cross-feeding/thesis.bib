	Journal = {Molecular Biology and Evolution},
	Journal1 = {Mol Biol Evol},
	Month = {9/12/2021},
	Number = {12},
	Pages = {2804--2816},
	Title = {Mammalian Evolution May not Be Strictly Bifurcating},
	Ty = {JOUR},
