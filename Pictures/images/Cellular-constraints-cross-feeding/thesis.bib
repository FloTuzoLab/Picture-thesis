@article{Zhou82,
	Annote = {doi: 10.1111/j.1432-1033.1982.tb06976.x},
	Author = {Zhou, Guo-Qiang and Zhong, Wei-Zhu},
	Booktitle = {European Journal of Biochemistry},
	Da = {1982/11/01},
