@article{Keren15,
	Annote = {10.1101/gr.191635.115},
	Author = {Keren, Leeat and van Dijk, David and Weingarten-Gabbay, Shira and Davidi, Dan and Jona, Ghil and Weinberger, Adina and Milo, Ron and Segal, Eran},
	Date = {2015/12/01},
	Date-Added = {2021-08-21 00:04:09 +0200},
	Date-Modified = {2021-08-21 00:04:22 +0200},
	Journal = {Genome Research},
	Journal1 = {Genome Research},
	Keywords = {noise; gene expression},
	Month = {12},
	Number = {12},
	Pages = {1893--1902},
	Title = {Noise in gene expression is coupled to growth rate},
	Ty = {JOUR},
	Url = {http://genome.cshlp.org/content/25/12/1893.abstract N2 - Genetically identical cells exposed to the same environment display variability in gene expression (noise), with important consequences for the fidelity of cellular regulation and biological function. Although population average gene expression is tightly coupled to growth rate, the effects of changes in environmental conditions on expression variability are not known. Here, we measure the single-cell expression distributions of approximately 900 Saccharomyces cerevisiae promoters across four environmental conditions using flow cytometry, and find that gene expression noise is tightly coupled to the environment and is generally higher at lower growth rates. Nutrient-poor conditions, which support lower growth rates, display elevated levels of noise for most promoters, regardless of their specific expression values. We present a simple model of noise in expression that results from having an asynchronous population, with cells at different cell-cycle stages, and with different partitioning of the cells between the stages at different growth rates. This model predicts non-monotonic global changes in noise at different growth rates as well as overall higher variability in expression for cell-cycle–regulated genes in all conditions. The consistency between this model and our data, as well as with noise measurements of cells growing in a chemostat at well-defined growth rates, suggests that cell-cycle heterogeneity is a major contributor to gene expression noise. Finally, we identify gene and promoter features that play a role in gene expression noise across conditions. Our results show the existence of growth-related global changes in gene expression noise and suggest their potential phenotypic implications.},
	Volume = {25},
	Year = {2015},
	Bdsk-Url-1 = {http://genome.cshlp.org/content/25/12/1893.abstract%20N2%20-%20Genetically%20identical%20cells%20exposed%20to%20the%20same%20environment%20display%20variability%20in%20gene%20expression%20(noise),%20with%20important%20consequences%20for%20the%20fidelity%20of%20cellular%20regulation%20and%20biological%20function.%20Although%20population%20average%20gene%20expression%20is%20tightly%20coupled%20to%20growth%20rate,%20the%20effects%20of%20changes%20in%20environmental%20conditions%20on%20expression%20variability%20are%20not%20known.%20Here,%20we%20measure%20the%20single-cell%20expression%20distributions%20of%20approximately%20900%20Saccharomyces%20cerevisiae%20promoters%20across%20four%20environmental%20conditions%20using%20flow%20cytometry,%20and%20find%20that%20gene%20expression%20noise%20is%20tightly%20coupled%20to%20the%20environment%20and%20is%20generally%20higher%20at%20lower%20growth%20rates.%20Nutrient-poor%20conditions,%20which%20support%20lower%20growth%20rates,%20display%20elevated%20levels%20of%20noise%20for%20most%20promoters,%20regardless%20of%20their%20specific%20expression%20values.%20We%20present%20a%20simple%20model%20of%20noise%20in%20expression%20that%20results%20from%20having%20an%20asynchronous%20population,%20with%20cells%20at%20different%20cell-cycle%20stages,%20and%20with%20different%20partitioning%20of%20the%20cells%20between%20the%20stages%20at%20different%20growth%20rates.%20This%20model%20predicts%20non-monotonic%20global%20changes%20in%20noise%20at%20different%20growth%20rates%20as%20well%20as%20overall%20higher%20variability%20in%20expression%20for%20cell-cycle%E2%80%93regulated%20genes%20in%20all%20conditions.%20The%20consistency%20between%20this%20model%20and%20our%20data,%20as%20well%20as%20with%20noise%20measurements%20of%20cells%20growing%20in%20a%20chemostat%20at%20well-defined%20growth%20rates,%20suggests%20that%20cell-cycle%20heterogeneity%20is%20a%20major%20contributor%20to%20gene%20expression%20noise.%20Finally,%20we%20identify%20gene%20and%20promoter%20features%20that%20play%20a%20role%20in%20gene%20expression%20noise%20across%20conditions.%20Our%20results%20show%20the%20existence%20of%20growth-related%20global%20changes%20in%20gene%20expression%20noise%20and%20suggest%20their%20potential%20phenotypic%20implications.}}

@article{Flamholz14,
	Abstract = {The microscopic world of a cell can be as alien to our human-centered intuition as the confinement of quarks within protons or the event horizon of a black hole. We are prone to thinking by analogy?Golgi cisternae stack like pancakes, red blood cells look like donuts?but very little in our human experience is truly comparable to the immensely crowded, membrane-subdivided interior of a eukaryotic cell or the intricately layered structures of a mammalian tissue. So in our daily efforts to understand how cells work, we are faced with a challenge: how do we develop intuition that works at the microscopic scale?},
