	Doi = {https://doi.org/10.1016/j.plrev.2021.03.004},
	Isbn = {1571-0645},
	Journal = {Physics of Life Reviews},
	Keywords = {Molecular evolution; Genotype-phenotype map; Genotype network; Fitness landscape; Phenotypic bias; Experimental evolution},
	Pages = {55--106},
	Title = {From genotypes to organisms: State-of-the-art and perspectives of a cornerstone in evolutionary dynamics},
	Ty = {JOUR},
