	Isbn = {1741-7007},
	J2 = {BMC Biol},
	Journal = {BMC biology},
	Keywords = {Animals; Arthropods/classification/*genetics; Biological Clocks/*genetics; Cell Nucleus/genetics; DNA, Mitochondrial/analysis; *Ecosystem; *Evolution, Molecular; Glyceraldehyde-3-Phosphate Dehydrogenases/genetics; Phosphopyruvate Hydratase/genetics; *Phylogeny; Sequence Analysis, DNA},
